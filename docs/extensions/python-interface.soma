import importlib

def call_builtin(vm, al, store, reg\29\:
    """FFI primitive - call any Python callable."""
    callable_name = al.pop(\29\

    # Collect arguments until Void
    args = []
    while True:
        val = al.pop(\29\
        if isinstance(val, VoidValue\29\:
            break
        args.append(val\29\

    # Reverse to match natural push order
    args.reverse(\29\

    # Resolve callable (support module.function syntax\29\
    try:
        parts = callable_name.split('.'\29\
        if len(parts\29\ == 1:
            # Built-in function
            func = eval(callable_name\29\
        else:
            # Module.function
            mod = importlib.import_module('.'.join(parts[:-1]\29\\29\
            func = getattr(mod, parts[-1]\29\

        # Call with converted args
        py_args = [_soma_to_python(arg\29\ for arg in args]
        result = func(*py_args\29\

        # Success: push result and Void exception
        al.push(_python_to_soma(result\29\\29\
        al.push(VoidValue(\29\\29\

    except Exception as e:
        # Failure: push Void result and exception
        al.push(VoidValue(\29\\29\
        al.push(ThingValue(e\29\\29\

def load_builtin(vm, al, store, reg\29\:
    """Load SOMA code from filesystem."""
    filepath = al.pop(\29\
    with open(filepath, 'r'\29\ as f:
        code = f.read(\29\
    vm._execute_code(code\29\

def import_builtin(vm, al, store, reg\29\:
    """Import a Python module."""
    module_name = al.pop(\29\
    try:
        importlib.import_module(module_name\29\
        al.push(BoolValue(True\29\\29\
    except ImportError:
        al.push(BoolValue(False\29\\29\

def register(vm\29\:
    """Register Python extension builtins."""
    vm.register_extension_builtin('use.python.call', call_builtin\29\
    vm.register_extension_builtin('use.python.load', load_builtin\29\
    vm.register_extension_builtin('use.python.import', import_builtin\29\

