class VM:
    def __init__(self, load_stdlib=True\29\:
        self.builtins = self._core_builtins(\29\
        self.builtins['use'] = self._use_builtin  # Only global addition
        self.loaded_extensions = set(\29\

        # Auto-load stdlib (100% pure SOMA\29\
        if load_stdlib:
            self._load_stdlib(\29\

    def _load_stdlib(self\29\:
        """Load stdlib.soma - pure SOMA, no extension dependencies."""
        stdlib_path = Path(__file__\29\.parent / 'stdlib.soma'
        with open(stdlib_path, 'r'\29\ as f:
            stdlib_code = f.read(\29\
        self._execute_code(stdlib_code\29\

    def _use_builtin(self, al, store, reg\29\:
        """Load an extension by name."""
        ext_name = al.pop(\29\
        if ext_name in self.loaded_extensions:
            return

        ext_module = importlib.import_module(f'soma.extensions.{ext_name}'\29\
        ext_module.register(self\29\

        # Run extension's SOMA setup code (creates Store macros\29\
        if hasattr(ext_module, 'get_soma_setup'\29\:
            setup_code = ext_module.get_soma_setup(\29\
            if setup_code:
                self._execute_code(setup_code\29\

        self.loaded_extensions.add(ext_name\29\

    def register_extension_builtin(self, name, func\29\:
        """Extensions call this - enforces use.* namespace."""
        if not name.startswith('use.'\29\:
            raise ValueError(f"Extension builtins must be under 'use.*': {name}"\29\
        self.builtins[name] = func) (python) >md.code

